{
    "name": "cirel/laravel-basics-auxs",
    "description": "Auxiliary library for Laravel projects to structure responses, requests, etc.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cirel\\LaravelBasicsAuxs\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Cirel H. Romeo",
            "email": "cirelhromeo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cirel\\LaravelBasicsAuxs\\LaravelBasicsAuxsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
